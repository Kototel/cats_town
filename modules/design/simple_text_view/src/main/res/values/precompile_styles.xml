<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <style name="SimpleTextViewPrecompile">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="SimpleTextViewPrecompile.SingleLine">
        <item name="android:text">Test text</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="SimpleTextViewPrecompile.MultiLine">
        <item name="android:text">Test text Test text Test text Test text \n Test text \n Test text Test text Test text Test text Test text</item>
        <item name="android:maxLines">2</item>
    </style>
    
</resources>